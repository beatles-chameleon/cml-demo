<view class="page-head  cml-base cml-view">
  <view class="page-head-title  cml-base cml-view">
    <text class="page-head-title-text  cml-base cml-text">{{title}}</text>
  </view>
  <view class="page-head-line  cml-base cml-view"></view>
  <view s-if="desc" class="page-head-desc  cml-base cml-view">
    <text class="page-head-desc-text  cml-base cml-text">{{desc}}</text>
  </view>
</view>