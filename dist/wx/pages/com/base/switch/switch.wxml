<cml-buildin-page title="switch演示" class=" cml-base cml-page">
  <c-header title="switch" class=" cml-view cml-c-header"></c-header>
    <view class="box  cml-base cml-view">
      <cml-buildin-switch checked="{{ switchValue1 }}" label="Switch" bindchange="_cmlEventProxy" data-eventchange="{{['switchChange1']}}" class=" cml-base cml-switch">
      </cml-buildin-switch>
    </view>
    <view class="title  cml-base cml-view">
      <text class=" cml-base cml-text">switch value: {{switchValue1}}</text>
    </view>
    <view class="box  cml-base cml-view">
      <cml-buildin-switch checked="{{ switchValue2 }}" label="Switch" bindchange="_cmlEventProxy" data-eventchange="{{['switchChange2']}}" class=" cml-base cml-switch">
      </cml-buildin-switch>
    </view>
    <view class="title  cml-base cml-view">
     <text class=" cml-base cml-text">switch value: {{switchValue2}}</text>
   </view>
    <view class="box  cml-base cml-view">
       <cml-buildin-switch checked="{{ switchValue3 }}" label="Disabled Switch" disabled="{{true}}" class=" cml-base cml-switch">
      </cml-buildin-switch>
    </view>
    <view class="title  cml-base cml-view">
      <text class=" cml-base cml-text">switch value: {{switchValue3}}</text>
    </view>
    <view class="box  cml-base cml-view">
       <cml-buildin-switch checked="{{ switchValue4 }}" label="Disabled Switch" disabled="{{true}}" class=" cml-base cml-switch">
      </cml-buildin-switch>
    </view>
    <view class="title  cml-base cml-view">
      <text class=" cml-base cml-text">switch value: {{switchValue4}}</text>
    </view>
</cml-buildin-page>