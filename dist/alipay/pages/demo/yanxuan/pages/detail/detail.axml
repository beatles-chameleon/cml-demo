            <text class="description-item-des-tag  cml-base cml-text cml-5c296b0f">{{item.tag2}}</text>
          </view>
        </view>
      </view>
      <view class="section  cml-base cml-view cml-5c296b0f">
        <view class="section-money  cml-base cml-view cml-5c296b0f">
          <text class="class-money  cml-base cml-text cml-5c296b0f">{{money}}</text>
        </view>
        <view class="section-tag  cml-base cml-view cml-5c296b0f" onTap="_cmlEventProxy" data-eventtap="{{['goto']}}">
          <text class="section-tag-con  cml-base cml-text cml-5c296b0f">{{appTag}}</text>
        </view>
        <view class="section-evaluate  cml-base cml-view cml-5c296b0f">
